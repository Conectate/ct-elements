{
	"printWidth": 200,
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxBracketSameLine": true,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"arrowParens": "avoid",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
	"plugins": [
		"@snelx/prettier-plugin-import-sort"
	]
}