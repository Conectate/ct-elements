{
	"compilerOptions": {
		"target": "es2017",
		"module": "es2015",
		"moduleResolution": "node",
		"lib": ["esnext.array", "esnext", "es2017", "dom"],
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"strict": true,
		"noUnusedLocals": true,
		"outDir": "./dist",
		// Only necessary because @types/uglify-js can't find types for source-map
		"skipLibCheck": true,
		"experimentalDecorators": true,
		"importHelpers": true
	},
	"exclude": []
}
