{
	"compilerOptions": {
		"target": "es2021",
		"module": "es2015",
		"moduleResolution": "node",
		"lib": [
			"esnext",
			"dom"
		],
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"strict": true,
		"noUnusedLocals": true,
		"outDir": "./",
		"skipLibCheck": true,
		"experimentalDecorators": true
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": []
}